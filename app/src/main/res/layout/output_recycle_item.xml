<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ll_tower_parameter_item"
    android:layout_width="55dp"
    android:layout_height="33dp"
    android:orientation="vertical">
    <LinearLayout
        android:layout_width="55dp"
        android:layout_height="33dp"
        android:background="@drawable/output_main_item_bg"
        android:orientation="vertical">
    <TextView
        android:id="@+id/tv_key"
        android:layout_width="match_parent"
        android:layout_height="13dp"
        android:textColor="@android:color/white"
        android:layout_marginStart="1px"
        android:layout_marginEnd="1px"
        android:gravity="center"
        android:background="@color/primary_background_color_blue"
        android:textSize="5dp"
        android:text="key"/>
    <RelativeLayout
        android:id="@+id/rl_output"
        android:layout_width="43dp"
        android:layout_height="11dp"
        android:background="@null"
        android:paddingEnd="0dp"
        android:layout_marginTop="5dp"
        android:layout_gravity="center">
        <Spinner
            android:id="@+id/spinner_output"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:spinnerMode="dropdown"
            android:popupBackground="@android:color/white"
            android:dropDownHorizontalOffset="0dp"
            android:dropDownVerticalOffset = "17dp"
            android:dropDownSelector="@drawable/setting_spinner_dropdown_bg_selector"
            android:background="@drawable/setting_edittext_bg"
            />
        <ImageView
            android:layout_width="7dp"
            android:layout_height="4dp"
            android:layout_centerVertical="true"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="3dp"
            android:src="@mipmap/xuanze"/>
    </RelativeLayout>
    </LinearLayout>

</LinearLayout>